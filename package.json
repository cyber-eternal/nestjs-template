{
  "name": "nestjs-template",
  "version": "4.0.0",
  "template-version": "4.0.0",
  "description": "Nest.js based template",
  "author": "cyber-eternal",
  "license": "MIT",
  "scripts": {
    "start": "npx ts-node -r tsconfig-paths/register src/main.ts",
    "start:debug": "nodemon --config nodemon-debug.json",
    "start:prod": "node dist/src/main",
    "start:docker": "npm run build && pm2 start ecosystem.config.js --no-daemon",
    "pm2:start": "npm run build && sudo pm2 start ecosystem.config.js",
    "pm2:stop": "sudo pm2 delete ecosystem.config.js",
    "test": "jest",
    "build": "nest build",
    "prebuild": "rimraf dist",
    "postinstall": "husky install",
    "pre-commit": "npx lint-staged",
    "prestart:prod": "rimraf dist && npm run build",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:generate": "npm run typeorm -- migration:generate -n",
    "migration:run": "npm run typeorm migration:run",
    "migration:revert": "npm run typeorm migration:revert",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "prettier": "prettier --check '*/**/*.{js,ts,tsx}'",
    "prettier:fix": "prettier --write '*/**/*.{js,ts,tsx}'"
  },
  "dependencies": {
    "@nestjs/apollo": "^10.1.3",
    "@nestjs/axios": "^0.1.0",
    "@nestjs/common": "^9.1.4",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.1.4",
    "@nestjs/graphql": "^10.1.3",
    "@nestjs/platform-express": "^9.1.4",
    "@nestjs/swagger": "^6.1.2",
    "@nestjs/typeorm": "^9.0.1",
    "@types/amqplib": "^0.8.2",
    "amqplib": "^0.10.3",
    "apollo-server-express": "^3.10.3",
    "async-retry": "^1.3.3",
    "aws-sdk": "^2.1237.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "express": "^4.18.2",
    "express-rate-limit": "^6.6.0",
    "graphql": "^16.6.0",
    "helmet": "^6.0.0",
    "joi": "^17.6.3",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "mysql": "^2.18.1",
    "nest-aws-sdk": "^3.0.0",
    "nestjs-redis": "^1.3.3",
    "nestjs-typeorm-paginate": "^4.0.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.7",
    "swagger-ui-express": "^4.5.0",
    "typeorm": "^0.3.10",
    "url-join": "^5.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@nestjs/cli": "^9.1.4",
    "@nestjs/schematics": "^9.0.3",
    "@nestjs/testing": "^9.1.4",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.2.0",
    "@types/jest-json-schema": "^6.1.1",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.11.3",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.3",
    "eslint-plugin-jsdoc": "^39.3.14",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "jest": "^29.2.1",
    "jest-json-schema": "^6.1.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "supertest": "^6.3.0",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.4",
    "yarn-upgrade-all": "^0.7.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      ".+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
